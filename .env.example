# Environment settings  
ENV=development  
DEBUG=true  
LOG_LEVEL=DEBUG  

# Project info
PROJECT_NAME="Rankyx"
PROJECT_DESCRIPTION="A production-ready FastAPI application"
VERSION=0.1.0

# API settings
API_PREFIX=/api
HOST=0.0.0.0
PORT=8000

# CORS settings
CORS_ORIGINS=["*"]
  
# PostgreSQL connection
POSTGRES_HOST=localhost  
POSTGRES_PORT=5432
POSTGRES_USER=postgres  
POSTGRES_PASSWORD=postgres  
POSTGRES_DB=app_db  
  
# Redis connection  
REDIS_HOST=localhost
REDIS_PORT=6300  
REDIS_PASSWORD=
REDIS_DB=0
  
# Dramatiq settings
DRAMATIQ_BROKER=redis
DRAMATIQ_PROCESSES=2
DRAMATIQ_THREADS=8

# Cache settings
CACHE_TTL_SECONDS=300  
CACHE_BACKEND_TYPE=memory
CACHE_FILE_PATH=cache

# Scheduler settings
SCHEDULER_ENABLED=true
